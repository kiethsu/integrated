    <script>
      
      const teamContainer = document.querySelector('.team2');
      const images = document.querySelectorAll('.team2 img');
      const imageWidth = 300; // Width of each image
      const gap = 80; // Gap between images
      const animationDuration = 8; // Total duration for the full loop in seconds
      
      let isPaused = false; // Flag to track if animation is paused
      
      // Set initial position of images
      images.forEach((image, index) => {
          image.style.left = `${(index * (imageWidth + gap)) + window.innerWidth}px`; // Start off-screen to the right
      });
      
      // Function to move images
      function moveImages() {
          if (isPaused) return; // Exit if paused
      
          images.forEach((image) => {
              // Get the current left position and calculate the new position
              const currentLeft = parseFloat(image.style.left);
              image.style.left = `${currentLeft - (imageWidth + gap) / (animationDuration * 60)}px`; // Move left
      
              // Check if the image is completely out of view
              if (currentLeft + imageWidth < 0) {
                  // Remove the transition temporarily
                  image.style.transition = 'none'; 
                  
                  // Move the current image to the end after the last visible image
                  image.style.left = `${getLastVisibleRightEdge()}px`;
      
                  // Restore the transition for smooth movement afterward
                  setTimeout(() => {
                      image.style.transition = ''; // Restore the default transition
                  }, 0);
              }
          });
      }
      
      // Function to get the right edge of the last visible image
      function getLastVisibleRightEdge() {
          let lastRightEdge = 0; // Start at 0 to track the rightmost edge
          images.forEach((image) => {
              const otherLeft = parseFloat(image.style.left);
              // Only consider images that are still on-screen (right of the left edge)
              if (otherLeft + imageWidth > 0) {
                  lastRightEdge = Math.max(lastRightEdge, otherLeft + imageWidth + gap); // Update to the right edge plus gap
              }
          });
          // Return the position for the new image to come in
          return lastRightEdge; // Ensure no extra gap added here
      }
      
      // Add hover effect to enlarge image smoothly
      images.forEach((image) => {
          image.addEventListener('mouseenter', () => {
              // Scale up the image
              image.style.transform = 'scale(1.1)';
              // Pause the movement for all images
              isPaused = true;
          });
      
          image.addEventListener('mouseleave', () => {
              // Reset scale
              image.style.transform = 'scale(1)';
              // Resume the movement for all images
              isPaused = false;
          });
      });
      
      // Start the animation loop
      function animate() {
          moveImages();
          requestAnimationFrame(animate);
      }
      
      // Start the animation
      animate();

