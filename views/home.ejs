<script>
  const form = document.getElementById('contact-form');

  form.addEventListener('submit', function(event) {
    event.preventDefault(); // Prevent form from submitting the traditional way

    // Collect form data
    const formData = new FormData(form);

    // Send data to the server using fetch
    fetch('/submit-contact', {
      method: 'POST',
      body: new URLSearchParams(formData),  // Convert FormData to URL-encoded string
      headers: {
        'Content-Type': 'application/x-www-form-urlencoded'  // Required for Express to parse body
      }
    })
    .then(response => response.json())
    .then(data => {
      if (data.success) {
        // Display success message (you can replace this with a better UI notification)
        alert(data.message);
        form.reset();  // Optional: Reset the form fields after successful submission
      } else {
        // Display error message
        alert('Error: ' + data.message);
      }
    })
    .catch(error => {
      console.error('Error:', error);
      alert('Well get on you as soon as possible.');
    });
  });
</script>

      

  
