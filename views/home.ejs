function copyToClipboard(text) {
    // Create a temporary textarea element
    const textarea = document.createElement('textarea');
    textarea.value = text; // Set the value to the text to be copied
    document.body.appendChild(textarea); // Append to the body
    textarea.select(); // Select the text
    document.execCommand('copy'); // Execute the copy command
    document.body.removeChild(textarea); // Remove the textarea
    alert('Copied to clipboard: ' + text); // Optional: alert to show success
}

